<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">
      <%= relationships.count %> <%= title_text %>
      <button type="button" class="collapsed panel-toggle" data-toggle="collapse" data-target=<%= "##{collapse_target}" %> aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="glyphicon glyphicon-triangle-bottom"></span>
      </button>
    </h3>
  </div>
  <div class="panel-body collapse in" id=<%= "#{collapse_target}" %>>
    <% relationships.each do |user| %>
        <%= link_to user_path(user), class: "card" do  %>
        <div class="my-restaurant">
          <div><%= image_tag 'http://via.placeholder.com/300x300' %></div> <!-- user avatar -->
          <p class="text-center"><%= user.name %></p>
        </div>
      <% end %>
    <% end %>
  </div>
</div>