<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= restaurants.count %> <%= title_text %></h3>
  </div>
  <div class="panel-body">
    <% restaurants.each do |restaurant| %>
        <%= link_to restaurant_path(restaurant), class: "card" do  %>
        <div class="my-restaurant">
          <div><%= show_restaurant_img(restaurant.image) %></div>
          <p class="text-center"><%= restaurant.name %></p>
        </div>
      <% end %>
    <% end %>
  </div>
</div>